{
  "name": "sass-graph-viz",
  "version": "1.0.0",
  "description": "Draw a visual graph of SASS dependencies",
  "keywords": [
    "sass",
    "dependencies",
    "graph",
    "visual",
    "dependency-graph",
    "dataviz"
  ],
  "bin": {
    "sassgraphviz": "./dist/cli.js",
    "sgv": "./dist/cli.js"
  },
  "author": "Nicolas Carlo <nicolascarlo.espeon@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "postbuild": "chmod +x ./dist/cli.js && npm run copy-static-files",
    "copy-static-files": "copyfiles -u 1 src/rendering/**/*.{js,css,pug} dist",
    "build:watch": "npm run prebuild && tsc-watch --onSuccess \"npm run postbuild\"",
    "test": "jest",
    "examples": "run-s examples:*",
    "examples:basic-tree": "ts-node examples/basic-tree",
    "examples:two-entry-points": "ts-node examples/two-entry-points",
    "examples:nested-folders": "ts-node examples/nested-folders",
    "examples:simple-viz": "ts-node examples/simple-viz"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/node": "10.9.4",
    "@types/pug": "2.0.4",
    "@types/sass-graph": "2.1.2",
    "@types/vis": "4.21.7",
    "copyfiles": "2.1.0",
    "jest": "23.5.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tsc-watch": "1.0.27",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "commander": "2.18.0",
    "express": "4.16.3",
    "lodash": "4.17.10",
    "opn": "5.3.0",
    "pug": "2.0.3",
    "sass-graph": "3.0.0",
    "viz.js": "2.0.0"
  }
}
