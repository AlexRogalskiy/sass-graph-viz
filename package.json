{
  "name": "sass-deps-graph",
  "version": "1.0.0",
  "description": "Draw graph of SASS dependencies",
  "keywords": [
    "sass",
    "dependencies",
    "graph"
  ],
  "bin": {
    "sassdeps": "./dist/cli.js"
  },
  "author": "Nicolas Carlo <nicolascarlo.espeon@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "postbuild": "chmod +x ./dist/cli.js",
    "build:watch": "npm run prebuild && tsc-watch --onSuccess \"npm run postbuild\"",
    "test": "jest",
    "examples": "run-s examples:*",
    "examples:basic-tree": "ts-node examples/basic-tree"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/node": "10.9.4",
    "@types/sass-graph": "2.1.2",
    "jest": "23.5.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tsc-watch": "1.0.27",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "commander": "2.18.0",
    "express": "4.16.3",
    "lodash": "4.17.10",
    "opn": "5.3.0",
    "sass-graph": "3.0.0",
    "viz.js": "2.0.0"
  }
}
